    }
  },
  "napa": {
    "threex.keyboardstate": "jeromeetienne/threex.keyboardstate"
  },
  "devDependencies": {
    "napa": "latest",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-imagemin": "^0.6.2",
    "gulp-notify": "^1.4.2",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-util": "^3.0.0",
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.8.0",
    "vinyl-source-stream": "~0.1.1",
    "event-stream": "latest",
    "watchify": "~1.0.2"
  },
  "jshintConfig": {
    "browserify": true,
    "asi": true,
    "devel": true
  }
}
